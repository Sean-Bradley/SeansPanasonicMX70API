<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="ComPort" value="COM4"/>
    <add key="BaudRate" value="38400"/>
    <add key="localAddress" value="https://192.168.80.181:90/MX70_API" />
    <add key="externalAddress" value="https://109.231.230.54:91/MX70_API" />
  </appSettings>
  <system.serviceModel>
    <client />
    <services>
      <service behaviorConfiguration="MX70_API_ServiceBehavior" name="MX70_API.WCF.SOAP.MX70_API_Service">
        <endpoint binding="basicHttpBinding" bindingConfiguration="MX70_API_Binding"
          bindingNamespace="MX70_API" contract="MX70_API.WCF.SOAP.IMX70_API_Service" />
        <host>
          <baseAddresses>
            <add baseAddress="" />
          </baseAddresses>
        </host>
      </service>
    </services>
    <bindings>
      <basicHttpBinding>
        <binding name="MX70_API_Binding">
          <security mode="Transport">
            <transport clientCredentialType="None">
              <extendedProtectionPolicy policyEnforcement="Never" />
            </transport>
          </security>
        </binding>
      </basicHttpBinding>
    </bindings>
    <!--For debugging purposes set the returnUnknownExceptionsAsFaults attribute to true-->
    <behaviors>
      <serviceBehaviors>
        <behavior name="MX70_API_ServiceBehavior">
          <serviceDebug includeExceptionDetailInFaults="true" />
          <!--<serviceMetadata httpGetEnabled="true" />-->
          <serviceMetadata httpsGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>